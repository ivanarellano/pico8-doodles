function _init()
	cls()
	draw_circ_graph({x=64,y=64})
end

function draw_circ_graph(off)
	local wid=48
	local dblwid=wid*2
	local x=off.x
	local y=off.y
	
	circ(x,y,wid*.8,5)
	draw_h_dash(
		{x=x-wid,y=y},dblwid,3
	)
	draw_v_dash(
		{x=x,y=y-wid},dblwid,3
	)
end
-->8
function draw_h_dash(off,ln,sz)
	for i=0,ln do
		pset(off.x+i,off.y,5)
	end
end

function draw_v_dash(off,ln,sz)
	local lines={}
	for i=0,ln do
		if (i%sz==0) then 
			add(lines,i)
		end
	end
	
