version 18
__lua__
function _init()
	start_angle=0
	end_angle=360
	circ_verts=100
	
	cls()
	circle(64,64,12)
end

function deg_to_tau(deg)
	return deg/360
end

function circle(offx,offy,rad)
	local verts={}
	local theta=deg_to_tau(start_angle)
	local arc_stride=(end_angle-start_angle)/circ_verts

	for i=0,circ_verts do
		local x=rad*cos(theta)
		local y=rad*sin(theta)
		
		add(verts, {x=x,y=y})
	 printh(x.." "..y)
		theta+=deg_to_tau(arc_stride)
	end
	
	color(5)
	line(offx+rad,offy,offx+rad,offy)
	for k,v in pairs(verts) do
		line(v.x+offx,v.y+offy)
	end
	printh("****‚¨áÔ∏èüÖæÔ∏è‚ô™‚ñë****")
end
